{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_data_flow_source",
						"type": "DatasetReference"
					},
					"name": "orderdata"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_csv_sink",
						"type": "DatasetReference"
					},
					"name": "output"
				}
			],
			"transformations": [
				{
					"name": "aggregate1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          C_ID as string,",
				"          C_NAME as string,",
				"          C_ADD as string,",
				"          C_CONTACT as string,",
				"          C_CREDIT_DAYS as string,",
				"          CJ_DATE as string,",
				"          SEX as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     inferDriftedColumnTypes: true,",
				"     ignoreNoFilesFound: false) ~> orderdata",
				"orderdata aggregate(groupBy(SEX),",
				"     C_NAME = count()) ~> aggregate1",
				"aggregate1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> output"
			]
		}
	}
}